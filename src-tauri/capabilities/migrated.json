{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-app-write",
    "fs:allow-app-write-recursive",
    "fs:allow-appcache-write",
    "fs:allow-appcache-write-recursive",
    "fs:allow-read-dir",
    "fs:allow-mkdir",
    "fs:allow-exists",
    {
      "identifier": "fs:scope",
      "allow": [
        "$APPDATA/**/*"
      ]
    },
    "shell:allow-open",
    "dialog:allow-open",
    "dialog:allow-save",
    "shell:default",
    "fs:default",
    "dialog:default"
  ]
}